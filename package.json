{
  "name": "ders-programi",
  "version": "0.1.4",
  "description": "Ders Programı",
  "author": {
    "name": "Ders Programi App"
  },
  "build": {
    "productName": "Ders Programı",
    "appId": "com.dersprogrami.app",
    "files": [
      "server/**/*",
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "database/**/*",
      "renderer/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    },
    "extraMetadata": {
      "main": "build/electron.js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "./",
  "private": true,
  "license": "MIT",
  "keywords": [
    "app",
    "lesson",
    "express",
    "javascript",
    "react",
    "sqlite",
    "typescript"
  ],
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.1.0"
  },
  "main": "public/electron.js",
  "proxy": "http://localhost:3001",
  "scripts": {
    "start-server": "nodemon --exec electron server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "build-front": "react-scripts build",
    "build-electron": "electron-builder",
    "build-script": "node ./buildScript",
    "build": "yarn build-front && yarn rebuild && yarn build-electron",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
    "start": "concurrently \"cross-env BROWSER=none yarn start-server\" \"yarn start-front\" \"wait-on http://localhost:3000 && electron .\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "5.0.0-alpha.114",
    "@mui/material": "^5.11.2",
    "@mui/styled-engine-sc": "^5.11.0",
    "@mui/styles": "^5.11.2",
    "@mui/x-data-grid": "^5.17.17",
    "@mui/x-date-pickers": "^5.0.12",
    "axios": "1.2.1",
    "body-parser": "1.20.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "electron-is-dev": "^2.0.0",
    "express": "4.18.2",
    "formik": "^2.2.9",
    "helmet": "6.0.1",
    "knex": "0.20.13",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.11.0",
    "react-router-dom": "^6.6.1",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "concurrently": "5.1.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "nodemon": "2.0.3",
    "react-scripts": "3.4.1",
    "sqlite3": "^5.1.4",
    "wait-on": "^7.0.1"
  }
}
